pipeline {
    agent any

        stage('Install Dependencies') {
            steps {
                sh 'npm install'
            }
        }

        stage('Test') {
            steps {
                echo "No tests yet!"
            }
        }

        stage('Deploy') {
            steps {
                sh 'pm2 restart newmyapp || pm2 start app.js --name newmyapp'
            }
        }
    }
}
